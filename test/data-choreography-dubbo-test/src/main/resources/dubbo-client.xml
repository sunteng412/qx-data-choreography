<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!--提供方信息-->
    <dubbo:application name="dubbo-client"/>

    <!--配置注册中心 "N/A"表示不需要注册中心 file表示可以短暂缓存到本地,防止每次都要去拿服务列表-->
    <!--    <dubbo:registry address="N/A"/>-->
    <dubbo:registry address="zookeeper://127.0.0.1:2181"/>

    <dubbo:reference id="activityInterface" interface="com.linqingxuan.datachoreography.test.dubbo.activity.ActivityInterface" check="true"
                     version="1.0.0"/>

    <dubbo:reference id="itemInterface" interface="com.linqingxuan.datachoreography.test.dubbo.item.ItemInterface" check="true"
                     version="1.0.0"/>

</beans>